{
  "name": "clocked-client",
  "homepage": "https://clocked-app.github.io/client/",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-standalone": "NODE_ENV=standalone run-p type-check \"build-only {@}\" --",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "vite --port 8080 --host",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "test:coverage": "vitest --run --coverage",
    "test:coverage-summary": "vitest --run --coverage.enabled --coverage.provider=v8 --coverage.reporter=json-summary --coverage.reporter=json",
    "test:coverage-lcov": "vitest --run --coverage.enabled --coverage.provider=v8 --coverage.reporter=lcov",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:watch": "vitest",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "pinia": "^2.1.7",
    "quasar": "^2.0.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.6.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-istanbul": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "cypress": "^13.6.1",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^27.0.5",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "1.33.0",
    "sass-loader": "^12.0.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vitest-sonar-reporter": "^1.0.0",
    "vls": "^0.8.5",
    "vue-cli-plugin-quasar": "~5.0.2",
    "vue-tsc": "^1.8.25"
  },
  "type": "module"
}
